<h1>Listing aircraft_statuses</h1>

<table>
  <tr>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @aircraft_statuses.each do |aircraft_status| %>
  <tr>
    <td><%= aircraft_status.description %></td>
    <td><%= link_to 'Show', aircraft_status %></td>
    <td><%= link_to 'Edit', edit_aircraft_status_path(aircraft_status) %></td>
    <td><%= link_to 'Destroy', aircraft_status, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

